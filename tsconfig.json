{
  "include": ["src", "tests", ".dumirc.ts", "*.ts"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": ["vitest/globals"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src"],
      "@eflag/captcha": ["src"],
      "@eflag/captcha/*": ["src/*", "*"]
    },
    "lib": ["DOM", "ESNext"]
  }
}
